{
  "scopeName": "source.enscript",
  "patterns": [
    {
      "name": "keyword.control.enscript",
      "match": "\\b(class|void|int|float|bool|static|return|new|if|else|for|while|switch|case)\\b"
    },
    {
      "name": "entity.name.class.enscript",
      "match": "\\b[A-Z][a-zA-Z0-9_]*\\b(?=\\s*\\{)"
    },
    {
      "name": "entity.name.function.enscript",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*(?=\\()"
    },
    {
      "name": "support.function.enscript",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\s*\\.\\s*[a-zA-Z_][a-zA-Z0-9_]*(?=\\()"
    },
    {
      "name": "variable.enscript",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*(?=\\s*=)"
    },
    {
      "name": "constant.numeric.enscript",
      "match": "\\b[0-9]+\\b"
    },
    {
      "name": "string.quoted.double.enscript",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "comment.line.double-slash.enscript",
      "begin": "//",
      "end": "$"
    }
  ]
}
